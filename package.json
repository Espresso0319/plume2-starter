{
  "name": "counter",
  "version": "1.0.0",
  "description": "plume2-starter",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "start": "npm run demo",
    "hello": "cross-env NODE_ENV=hello ./node_modules/.bin/webpack-dev-server",
    "demo": "cross-env NODE_ENV=demo ./node_modules/.bin/webpack-dev-server",
    "clean": "rm -rf ./dist/*",
    "build:dll": "./node_modules/.bin/webpack --config webpack.dll.config.js --progress --color",
    "build": "./node_modules/.bin/webpack --config webpack.production.js --progress --color",
    "build:all": "npm run clean && npm run build:dll && npm run build",
    "dist": "python -m SimpleHTTPServer 3000"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [
    "plume2",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hufeng/plume2-starter.git"
  },
  "author": "hufeng",
  "license": "BSD",
  "dependencies": {
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.10",
    "@types/react-dom": "^0.14.23",
    "plume2": "^0.0.6",
    "preact": "^7.2.0",
    "preact-compat": "^3.13.1",
    "rc-list-view": "^0.0.3",
    "react-infinite-scroller": "^1.0.7",
    "react-router": "^3.0.2",
    "react-toolbox": "2.0.0-beta.4",
    "tslib": "^1.6.0"
  },
  "devDependencies": {
    "@types/classnames": "^0.0.32",
    "@types/jest": "^19.2.2",
    "@types/react-router": "^3.0.6",
    "@types/react-test-renderer": "^15.4.2",
    "@types/whatwg-fetch": "^0.0.33",
    "awesome-typescript-loader": "^3.0.8",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-syntax-trailing-function-commas": "^6.20.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-3": "6.17.0",
    "classnames": "^2.2.5",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.2",
    "file-loader": "^0.10.1",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "2.24.1",
    "jest": "^19.0.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-each": "^0.9.3",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.2",
    "postcss-mixins": "^5.4.1",
    "preact-async-route": "^1.0.3",
    "preact-router": "^2.4.1",
    "rc-css-transition-group": "^2.1.4",
    "react-style-proptype": "^2.0.1",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.2",
    "ts-jest": "^19.0.1",
    "ts-loader": "^2.0.1",
    "typescript": "2.2.0",
    "url-loader": "^0.5.8",
    "webpack": "2.2.0",
